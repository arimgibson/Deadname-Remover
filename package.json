{
  "name": "Deadname Remover",
  "version": "2.0.4",
  "description": "An easy to use browser plugin to automatically remove and replace deadnames",
  "private": true,
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arimgibson/Deadname-Remover.git"
  },
  "keywords": [
    "trans",
    "transgender",
    "dead",
    "name",
    "deadname",
    "lgbtq",
    "lgbtq+",
    "queer",
    "non-binary",
    "gender"
  ],
  "author": "Ari Gibson",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/arimgibson/Deadname-Remover/issues"
  },
  "homepage": "https://github.com/arimgibson/Deadname-Remover#readme",
  "scripts": {
    "dev": "wxt",
    "dev:firefox": "wxt -b firefox",
    "build": "wxt build",
    "build:firefox": "wxt build -b firefox",
    "zip": "wxt zip",
    "zip:firefox": "wxt zip -b firefox",
    "lint": "eslint .",
    "check": "svelte-check --tsconfig ./tsconfig.json && tsc --noEmit",
    "test": "vitest",
    "test:e2e": "bun run build && playwright test",
    "test:e2e:ui": "bun run build && playwright test --ui",
    "test:e2e:setup": "playwright install chromium",
    "compile": "tsc --noEmit",
    "postinstall": "wxt prepare"
  },
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "@iconify-json/material-symbols": "^1.2.16",
    "@iconify-json/twemoji": "^1.2.2",
    "@onu-ui/preset": "^1.1.5",
    "@playwright/test": "^1.51.0",
    "@stylistic/eslint-plugin": "^4.2.0",
    "@types/chrome": "^0.0.309",
    "@typescript-eslint/parser": "^8.26.1",
    "@wxt-dev/module-svelte": "^2.0.3",
    "@wxt-dev/unocss": "^1.0.1",
    "eslint": "^9.22.0",
    "eslint-plugin-svelte": "^3.1.0",
    "globals": "^16.0.0",
    "svelte": "^5.23.0",
    "svelte-check": "^4.1.5",
    "svelte-eslint-parser": "^1.0.1",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.26.1",
    "unocss": "^66.1.0-beta.5",
    "vitest": "^3.0.8",
    "wxt": "^0.19.29"
  },
  "dependencies": {
    "compare-versions": "^6.1.1",
    "microdiff": "^1.5.0",
    "svelte-french-toast": "^1.2.0",
    "text-security": "^3.2.1",
    "valibot": "^1.0.0-rc.4"
  }
}
